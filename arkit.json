{
	"$schema": "https://arkit.pro/schema.json",
	"excludePatterns": [
		"test/**",
		"tests/**",
		"**/*.test.*",
		"**/*.spec.*",
		"**/node_modules/**",
		"**/public/**"
	],
	"components": [
		{
			"type": "Composables",
			"patterns": [
				"composables/src/**/*.ts"
			]
		}
	],
	"output": [
		{
			"path": "composables/public/arkit.svg",
			"groups": [
				{
					"first": true,
					"components": [
						"Composables"
					]
				}
			]
		}
	]
}