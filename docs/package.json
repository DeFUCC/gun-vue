{
  "name": "@gun-vue/site",
  "private": true,
  "version": "0.5.8",
  "description": "test vitepress site for gun-vue",
  "main": "index.js",
  "scripts": {
    "dev": "npm run doc && vitepress dev",
    "doc": "jsdoc2md --no-gfm --template templates/docs.hbs --partial templates/separator.hbs --files ../composables/src/*.js > ./composables/index.md ",
    "build": "npm run doc && vitepress build",
    "serve": "vitepress serve --port=5500"
  },
  "keywords": [
    "vitepress",
    "gun",
    "gun-vue"
  ],
  "author": "davay42",
  "license": "MIT",
  "dependencies": {
    "@gun-vue/components": "workspace:^",
    "@vueuse/core": "7.5.3",
    "vue": "^3.2.27"
  },
  "devDependencies": {
    "@iconify/json": "^2.0.25",
    "jsdoc-to-markdown": "^7.1.0",
    "markdown-it-classy": "^0.2.0",
    "markdown-it-container": "^3.0.0",
    "markdown-it-external-links": "^0.0.6",
    "pug": "3.0.2",
    "unplugin-auto-import": "^0.5.11",
    "unplugin-icons": "^0.13.0",
    "unplugin-vue-components": "^0.17.13",
    "vite": "^2.7.12",
    "vite-aliases": "^0.8.7",
    "vite-plugin-windicss": "^1.6.3",
    "vitepress": "0.21.5",
    "vitepress-tags": "^0.4.0",
    "windicss": "^3.4.3"
  }
}
