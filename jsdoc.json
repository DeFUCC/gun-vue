{
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "include": ["."],
    "exclude": ["node_modules"],
    "excludePattern": ".*/node_modules.*"
  },
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "opts": {
    "destination": "./demo/public/docs/",
    "recurse": true,
    "readme": "README.md",
    "template": "node_modules/minami"
  }
}
