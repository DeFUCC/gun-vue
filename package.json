    "express": "^4.17.1",
    "gun": "^0.2020.1235",
    "gun-avatar": "^1.3.5",
    "ola": "^1.2.1",
    "vue": "^3.2.16",
    "vue-router": "^4.0.12",
    "gun-vue-composables": "workspace:*"
  },
  "devDependencies": {
    "@iconify/json": "^1.1.432",
    "@vitejs/plugin-vue": "^1.9.3",
    "pug": "3.0.2",
    "unplugin-auto-import": "^0.4.14",
    "unplugin-icons": "^0.12.18",
    "unplugin-vue-components": "^0.17.2",
    "vite": "^2.6.4",
    "vite-plugin-pages": "^0.19.0-beta.6",
    "vite-plugin-windicss": "^1.5.1",
    "windicss": "^3.2.1"
  }
