	},
	"type": "module",
	"scripts": {
		"app": "cd app && pnpm tauri dev",
		"dev": "cd app && pnpm run dev",
		"docs": "cd docs && pnpm run dev",
		"build": "cd composables && pnpm run lib-build && cd .. && cd docs && pnpm run build && cd .. && cd components && pnpm run build && cd .. && cd app && pnpm run build ",
		"preview": "cd app && pnpm run preview",
		"relay": "cd relay && pnpm run start",
		"tauri": "cd app && pnpm tauri build",
		"deploy": "sh deploy.sh",
		"change": "pnpm changeset",
		"version": "pnpm changeset version",
		"publish": "pnpm run build && pnpm changeset publish"
	},
	"devDependencies": {
		"@changesets/cli": "^2.26.1",
