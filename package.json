{
	"name": "@gun-vue/repo",
	"version": "0.15.0",
	"private": true,
	"homepage": "https://gun-vue.js.org",
	"funding": {
		"type": "github",
		"url": "https://github.com/sponsors/davay42"
	},
	"type": "module",
	"scripts": {
		"app": "pnpm --filter app run td",
		"dev": "pnpm --filter app run dev",
		"docs": "pnpm --filter docs run dev",
		"builds": "pnpm -filter=!app -r --parallel run build && pnpm -filter app run build",
		"build-line": "pnpm -r run build",
		"preview": "pnpm --filter app run preview",
		"relay": "pnpm --filter relay run start",
		"tauri": "pnpm --filter app run tb",
		"deploy": "sh deploy.sh",
		"change": "pnpm changeset",
		"version": "pnpm changeset version",
		"publish": "pnpm run builds && pnpm changeset publish"
	},
	"devDependencies": {
		"@changesets/cli": "^2.26.1",
		"@skynetlabs/skynet-nodejs": "^2.9.0",
		"@typescript-eslint/eslint-plugin": "^5.59.0",
		"@typescript-eslint/parser": "^5.59.0",
		"@volar/vue-language-plugin-pug": "1.2.0",
		"cli-progress": "3.12.0",
		"dotenv": "^16.0.3",
		"eslint": "^8.38.0",
		"eslint-plugin-vue": "^9.11.0",
		"eslint-plugin-vue-pug": "^0.6.0",
		"typedoc": "0.24.4",
		"typedoc-plugin-markdown": "^3.15.1",
		"typescript": "^5.0.4",
		"vite": "^4.2.2",
		"vue-eslint-parser": "^9.1.1"
	}
}
