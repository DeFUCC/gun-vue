#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/esbuild@0.13.15/node_modules/esbuild/bin/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/esbuild@0.13.15/node_modules/esbuild/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/esbuild@0.13.15/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules:/Users/davay/Documents/GitHub/node_modules:/Users/davay/Documents/node_modules:/Users/davay/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/esbuild@0.13.15/node_modules/esbuild/bin/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/esbuild@0.13.15/node_modules/esbuild/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/esbuild@0.13.15/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules:/Users/davay/Documents/GitHub/node_modules:/Users/davay/Documents/node_modules:/Users/davay/node_modules:/Users/node_modules:/node_modules"
fi
"$basedir/../../bin/esbuild"   "$@"
exit $?
