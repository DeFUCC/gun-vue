#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/vite@2.6.14/node_modules/vite/bin/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/vite@2.6.14/node_modules/vite/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/vite@2.6.14/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules:/Users/davay/Documents/GitHub/node_modules:/Users/davay/Documents/node_modules:/Users/davay/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/vite@2.6.14/node_modules/vite/bin/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/vite@2.6.14/node_modules/vite/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/vite@2.6.14/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules:/Users/davay/Documents/GitHub/node_modules:/Users/davay/Documents/node_modules:/Users/davay/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi
