#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/@unocss+cli@0.12.2/node_modules/@unocss/cli/bin/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/@unocss+cli@0.12.2/node_modules/@unocss/cli/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/@unocss+cli@0.12.2/node_modules/@unocss/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/@unocss+cli@0.12.2/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules:/Users/davay/Documents/GitHub/node_modules:/Users/davay/Documents/node_modules:/Users/davay/node_modules:/Users/node_modules:/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/unocss@0.12.2/node_modules/unocss/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/unocss@0.12.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/@unocss+cli@0.12.2/node_modules/@unocss/cli/bin/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/@unocss+cli@0.12.2/node_modules/@unocss/cli/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/@unocss+cli@0.12.2/node_modules/@unocss/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/@unocss+cli@0.12.2/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules:/Users/davay/Documents/GitHub/node_modules:/Users/davay/Documents/node_modules:/Users/davay/node_modules:/Users/node_modules:/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/unocss@0.12.2/node_modules/unocss/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/unocss@0.12.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@unocss+cli@0.12.2/node_modules/@unocss/cli/bin/unocss.js" "$@"
else
  exec node  "$basedir/../../../../../@unocss+cli@0.12.2/node_modules/@unocss/cli/bin/unocss.js" "$@"
fi
