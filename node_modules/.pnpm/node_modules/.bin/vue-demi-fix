#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/vue-demi@0.12.1_vue@3.2.22/node_modules/vue-demi/bin/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/vue-demi@0.12.1_vue@3.2.22/node_modules/vue-demi/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/vue-demi@0.12.1_vue@3.2.22/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules:/Users/davay/Documents/GitHub/node_modules:/Users/davay/Documents/node_modules:/Users/davay/node_modules:/Users/node_modules:/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/node_modules/vue-demi/bin/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/node_modules/vue-demi/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/vue-demi@0.12.1_vue@3.2.22/node_modules/vue-demi/bin/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/vue-demi@0.12.1_vue@3.2.22/node_modules/vue-demi/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/vue-demi@0.12.1_vue@3.2.22/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules:/Users/davay/Documents/GitHub/node_modules:/Users/davay/Documents/node_modules:/Users/davay/node_modules:/Users/node_modules:/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/node_modules/vue-demi/bin/node_modules:/Users/davay/Documents/GitHub/vite-gun-starter/node_modules/.pnpm/node_modules/vue-demi/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vue-demi/bin/vue-demi-fix.js" "$@"
else
  exec node  "$basedir/../vue-demi/bin/vue-demi-fix.js" "$@"
fi
